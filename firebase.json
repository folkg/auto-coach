{
  "hosting": {
    "public": "dist/auto-gm",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "/api/fetchuserteams",
        "run": {
          "serviceId": "lineup-fetchuserteams",
          "region": "us-central1"
        }
      },
      {
        "source": "/api/sendfeedbackemail",
        "run": {
          "serviceId": "email-sendfeedbackemail",
          "region": "us-central1"
        }
      }
    ]
  }
}
